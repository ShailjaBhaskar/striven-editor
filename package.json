{
  "name": "@striven-erp/striven-editor",
  "private": false,
  "version": "2.7.0",
  "description": "The lightweight zero-dependency Striven WYSIWYG editor.",
  "main": "./dist",
  "scripts": {
    "start": "cd demo && npm run serve",
    "prepare": "npm run build",
    "build": "npx webpack --mode=\"production\"",
    "dev": "npx webpack -w",
    "meta-server": "node metaserver",
    "gh-pages": "cd demo && npm run build && cd .. && node gh-pages",
    "new-publish": "npm publish --access=public && echo DON'T FORGET TO UPDATE GITHUB RELEASE"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/business-by-miles/striven-editor.git"
  },
  "keywords": [
    "Striven",
    "Editor",
    "WYSIWYG",
    "rich-text-editor"
  ],
  "author": "Striven <https://striven.com/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/business-by-miles/striven-editor/issues"
  },
  "homepage": "https://github.com/business-by-miles/striven-editor#readme",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-modules-umd": "^7.2.0",
    "@babel/preset-env": "^7.6.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "gh-pages": "^2.1.1",
    "got": "^9.6.0",
    "metascraper": "^5.7.4",
    "metascraper-description": "^5.7.4",
    "metascraper-image": "^5.7.4",
    "metascraper-title": "^5.7.4",
    "metascraper-url": "^5.7.4",
    "shortid": "^2.2.15",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.0.1",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {}
}
